function GrobalSettingsLibXP:GetThemeNames()
    local names = {}
    for name, value in pairs(self) do
        if type(value) == "userdata" and typeof(value) == "Color3" then
            table.insert(names, name)
        end
    end
    table.sort(names)
    return names
end

function GrobalSettingsLibXP:GetRandomTheme()
    local names = self:GetThemeNames()
    return names[math.random(1, #names)]
end

function GrobalSettingsLibXP:ApplyTheme(themeName, discordLib)
    if self[themeName] and discordLib and discordLib.SetTheme then
        discordLib:SetTheme(self[themeName])
        return true
    end
    return false
end

function GrobalSettingsLibXP:GetThemesByCategory(category)
    local results = {}
    for name, value in pairs(self) do
        if type(value) == "userdata" and typeof(value) == "Color3" and name:lower():find(category:lower()) then
            table.insert(results, name)
        end
    end
    table.sort(results)
    return results
end

function GrobalSettingsLibXP:CreateSettingsInChannel(channel, discordLib)
    if not channel then
        return
    end
    
    channel:Label("THEME SETTINGS")
    channel:Seperator()
    
    local themeNames = self:GetThemeNames()
    channel:Dropdown("Select Theme", themeNames, function(selectedTheme)
        self:ApplyTheme(selectedTheme, discordLib)
        discordLib:Notification({
            title = "Theme Changed",
            desc = "Theme: " .. selectedTheme,
            time = 2
        })
    end)
    
    channel:Label("QUICK TOPICS")
    channel:Button("Dark Theme", function()
        self:ApplyTheme("DiscordDark", discordLib)
        discordLib:Notification({title = "Dark Theme", desc = "Activado", time = 1.5})
    end)
    
    channel:Button("Clear Theme", function()
        self:ApplyTheme("DiscordLight", discordLib)
        discordLib:Notification({title = "Clear Theme", desc = "Activado", time = 1.5})
    end)
    
    channel:Button("AMOLED Theme", function()
        self:ApplyTheme("Black", discordLib)
        discordLib:Notification({title = "AMOLED Theme", desc = "Activado", time = 1.5})
    end)
    
    channel:Button("Random Theme", function()
        local randomTheme = self:GetRandomTheme()
        self:ApplyTheme(randomTheme, discordLib)
        discordLib:Notification({title = "Random Theme", desc = randomTheme, time = 1.5})
    end)
    
    channel:Seperator()
    channel:Label("Color Personalizado RGB")
    
    local r, g, b = 32, 34, 37
    
    channel:Slider("Rojo", 0, 255, r, function(value)
        r = value
        discordLib:SetTheme(Color3.fromRGB(r, g, b))
    end)
    
    channel:Slider("Verde", 0, 255, g, function(value)
        g = value
        discordLib:SetTheme(Color3.fromRGB(r, g, b))
    end)
    
    channel:Slider("Azul", 0, 255, b, function(value)
        b = value
        discordLib:SetTheme(Color3.fromRGB(r, g, b))
    end)
    
    channel:Button("Resetear", function()
        r, g, b = 32, 34, 37
        discordLib:SetTheme(Color3.fromRGB(r, g, b))
    end)
    
    return channel
end
    if ventanaExistente then
        local serverName = "ConfigServer"
        ventanaExistente.Server:Server(serverName, "")
        local configChannel = ventanaExistente:Channel(serverName, "Ajustes")
        return self:CreateSettingsInChannel(configChannel, discordLib)
    else
        local ventana = discordLib:Window("Configuraci√≥n Global")
        local serverName = "ConfigServer"
        ventana.Server:Server(serverName, "")
        local configChannel = ventana:Channel(serverName, "Ajustes")
        return self:CreateSettingsInChannel(configChannel, discordLib)
    end
end

return GrobalSettingsLibXP
