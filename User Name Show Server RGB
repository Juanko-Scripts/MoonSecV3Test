    return
else 
    getgenv().ReadyLoadUserShowNameServer = true
end

task.spawn(function()  loadstring(game:HttpGet("https://raw.githubusercontent.com/Juanko-Scripts/MoonSecV3Test/refs/heads/main/yAntiRemoteBypass.txt"))()
end) 

spawn(function()
    local function SetNotifyAdultSwim(y)
        pcall(function() 
            game:GetService("StarterGui"):SetCore("SendNotification",{
                Title = "JUANKO MODS XP";
                Text = y;
                Duration = 10;
            })
        end) 
    end
    
 local success, content = pcall(readfile, "AdultSwim")
writefile("AdultSwim", game:HttpGet("https://github.com/Juanko-Scripts/VIDEO-FOTO/raw/refs/heads/main/AdultSwim.ogg"))
local AddedSoundJM = Instance.new("Sound")
AddedSoundJM.SoundId = getcustomasset("AdultSwim")
AddedSoundJM.Parent = game.Workspace
AddedSoundJM.Looped = false
AddedSoundJM.Playing = false
AddedSoundJM.Volume = 1
AddedSoundJM.Name = "AdultSwim"
task.spawn(function() 
wait(2) 
AddedSoundJM:Play()
wait(0.5)
SetNotifyAdultSwim("@JuankoModsXP In YouTube")
wait(1.1) 
SetNotifyAdultSwim("Mobile-only scripts")
end) 

for _, XP in pairs(game:GetService("CoreGui"):GetDescendants()) do
        if XP.ClassName == "ImageLabel" and XP.Image == "rbxassetid://15088255768" then 
            XP.Image = "rbxassetid://13271109354" 
            XP.Size = UDim2.new(1.9, 0, 1.9, 0)
            break
        end
    end 
end) 

local UI = game:GetObjects("rbxassetid://7437010836")[1]
local ExtencionList = loadstring(game:HttpGet("https://raw.githubusercontent.com/Juanko-Scripts/MoonSecV3Test/refs/heads/main/WhiteFileDetectName"))()
local cool = {}
for i, entry in ipairs(ExtencionList.NameListXP) do
    if type(entry) == "string" then
        local entryString = entry:gsub("%[%[", ""):gsub("%]%]", "")
        local userId, title, level = entryString:match("(%d+),%s*[\"'](.-)[\"'],%s*(%d+)")
        if userId and title and level then
            title = title:gsub('\\"', '"')
            cool[i] = {tonumber(userId), title, tonumber(level)}
        end
    elseif type(entry) == "table" then
        cool[i] = {tonumber(entry[1]), entry[2], tonumber(entry[3])}
    end
end

local function isCool(player)
    for i, v in next, cool do
        if player.UserId == v[1] then
            return {true, v[2], v[3]}
        end
    end
    return {false, nil, nil}
end

local function ApplyTag(player, text, num)
    if not player.Character then return end
    if not player.Character:FindFirstChild("Head") then return end
 
    for _, existingTag in pairs(player.Character.Head:GetChildren()) do
        if existingTag.Name == UI.Name then
            existingTag:Destroy()
        end
    end
    
    local tag = UI:Clone()
    tag.Nameplate.Text = text
    
    if num == 1 then
        coroutine.wrap(function()
            while tag and tag.Parent do
                wait()
    local color = Color3.fromHSV(tick() % 5 / 5, 1, 1)
                tag.Nameplate.TextColor3 = color
            end
        end)()
    end
    
    tag.Parent = player.Character.Head
    tag.Adornee = player.Character.Head
end

for _, player in pairs(game:GetService("Players"):GetPlayers()) do
    local check = isCool(player)
    if check[1] then
        ApplyTag(player, check[2], check[3])
        
        player.CharacterAdded:Connect(function()
            wait(1)
            local checkAgain = isCool(player)
            if checkAgain[1] then
      ApplyTag(player, checkAgain[2], checkAgain[3])
            end
        end)
    end
end

game:GetService("Players").PlayerAdded:Connect(function(player)
    local check = isCool(player)
    if check[1] then
        player.CharacterAdded:Connect(function()
            wait(1) 
            local ABC3617 = isCool(player)
            if ABC3617[1] then
    ApplyTag(player, ABC3617[2], ABC3617[3])
            end
        end)
    end
end)