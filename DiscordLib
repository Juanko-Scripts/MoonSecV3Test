function DiscordLib:MinimizeUI()if minimizeAnimating then return end minimizeAnimating=true local mf=Discord:FindFirstChild("MainFrame")if not mf then minimizeAnimating=false return end if not isMinimized then currentSize,currentPos=mf.Size,mf.Position end isMinimized=not isMinimized local ts,tp=isMinimized and minimizedSize or originalSize,isMinimized and minimizedPosition or currentPos local sf,cf,up=mf:FindFirstChild("ServersHoldFrame"),mf:FindFirstChild("ChannelsHoldFrame"),mf:FindFirstChild("Userpad",true)if isMinimized then TweenService:Create(mf,TweenInfo.new(0.3,Enum.EasingStyle.Quad),{Size=ts,Position=tp}):Play()task.wait(0.15)if sf then sf.Visible=false end if cf then cf.Visible=false end if up then up.Visible=false end for _,s in pairs(Sections)do if s and s.Parent then s.Visible=false end end local t=mf:FindFirstChild("Title",true)if t then t.Text="[Minimized]"end else if sf then sf.Visible=true end if cf then cf.Visible=true end if up then up.Visible=true end local fn=currentServer..":"..currentChannel for n,s in pairs(Sections)do if s and s.Parent then s.Visible=(n==fn)end end local t=mf:FindFirstChild("Title",true)if t then t.Text="Discord UI"end TweenService:Create(mf,TweenInfo.new(0.3,Enum.EasingStyle.Quad),{Size=ts,Position=tp}):Play()end task.wait(0.3)minimizeAnimating=false end
