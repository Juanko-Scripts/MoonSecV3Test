local Servers,ServerChannels={},{}function DiscordLib:SetTheme(tn)local nt if type(tn)=="string"then nt=Themes[tn]or Themes.Dark else nt={Main=tn,Secondary=tn:lerp(Color3.new(0,0,0),0.2),Tertiary=tn:lerp(Color3.new(0,0,0),0.3),Accent=tn:lerp(Color3.new(1,1,1),0.3),Text=tn:lerp(Color3.new(1,1,1),0.8),TextMuted=tn:lerp(Color3.new(1,1,1),0.5),Userpad=tn:lerp(Color3.new(0,0,0),0.4),UserIcon=tn:lerp(Color3.new(0,0,0),0.5),Hover=tn:lerp(Color3.new(1,1,1),0.2),Border=tn:lerp(Color3.new(0,0,0),0.8)}end currentTheme=nt for _,f in ipairs(ThemeUpdateConnections)do pcall(f)end end
local function rtu(f)table.insert(ThemeUpdateConnections,f)end
rtu(function()MainFrame.BackgroundColor3,TopFrame.BackgroundColor3,TopFrameHolder.BackgroundColor3,Title.TextColor3=currentTheme.Main,currentTheme.Main,currentTheme.Main,currentTheme.TextMuted end)rtu(function()ServersHoldFrame.BackgroundColor3=currentTheme.Secondary for _,b in ipairs(ServersHold:GetChildren())do if b:IsA("TextButton")then b.BackgroundColor3=currentTheme.Tertiary local i=b:FindFirstChild("ServerIcon")if i then i.ImageColor3=currentTheme.Text end end end end)rtu(function()ChannelsHoldFrame.BackgroundColor3=currentTheme.Secondary for _,b in ipairs(ChannelsHold:GetChildren())do if b:IsA("TextButton")then b.BackgroundColor3=b.BackgroundTransparency<0.5 and currentTheme.Accent or currentTheme.Tertiary local i=b:FindFirstChild("ChannelIcon")if i then i.TextColor3=currentTheme.TextMuted end for _,c in ipairs(b:GetChildren())do if c:IsA("TextLabel")and c~=i then c.TextColor3=currentTheme.Text end end end end end)rtu(function()Userpad.BackgroundColor3,UserIcon.BackgroundColor3,UserName.TextColor3,UserTag.TextColor3,UserCircleImage.ImageColor3=currentTheme.Userpad,currentTheme.UserIcon,currentTheme.Text,currentTheme.Text,currentTheme.Userpad end)
