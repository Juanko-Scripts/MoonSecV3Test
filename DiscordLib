function DiscordLib:CreateAcrylicBlur()local e=false;local i=0.5;local ne=true;local af,ac,mc=nil;local RS=game:GetService("RunService");local cAF=function()if af then return end;local dg=game:GetService("CoreGui"):FindFirstChild("Discord")if not dg then return end;local mf=dg:FindFirstChild("MainFrame")if not mf then return end;af=Instance.new("Frame");af.Name="AcrylicBlur";af.Parent=dg;af.BackgroundColor3=Color3.fromRGB(30,30,30);af.BackgroundTransparency=0.85;af.BorderSizePixel=0;af.Size=mf.Size;af.Position=mf.Position;af.AnchorPoint=mf.AnchorPoint;af.ZIndex=0;af.Active=false;af.ClipsDescendants=true;local c=Instance.new("UICorner");c.CornerRadius=UDim.new(0,4);c.Parent=af;local g=Instance.new("UIGradient");g.Color=ColorSequence.new({ColorSequenceKeypoint.new(0,Color3.fromRGB(40,40,40)),ColorSequenceKeypoint.new(1,Color3.fromRGB(20,20,20))});g.Rotation=90;g.Parent=af;local n=Instance.new("ImageLabel");n.Name="Noise";n.Parent=af;n.BackgroundColor3=Color3.fromRGB(255,255,255);n.BackgroundTransparency=1;n.BorderSizePixel=0;n.Size=UDim2.new(1,0,1,0);n.Position=UDim2.new(0,0,0,0);n.ZIndex=1;n.Image="rbxassetid://145514428";n.ImageColor3=Color3.fromRGB(255,255,255);n.ImageTransparency=0.96;n.ScaleType=Enum.ScaleType.Tile;n.TileSize=UDim2.new(0,128,0,128);mc=mf:GetPropertyChangedSignal("Position"):Connect(function()if af and af.Parent then af.Position=mf.Position end end);mf:GetPropertyChangedSignal("Size"):Connect(function()if af and af.Parent then af.Size=mf.Size end end);mf:GetPropertyChangedSignal("Visible"):Connect(function()if af and af.Parent then af.Visible=mf.Visible end end);mf:GetPropertyChangedSignal("AnchorPoint"):Connect(function()if af and af.Parent then af.AnchorPoint=mf.AnchorPoint end end)end;local uA=function()if ac then ac:Disconnect()end;ac=RS.RenderStepped:Connect(function()if af and af.Parent and e then local n=af:FindFirstChild("Noise")if n then n.ImageTransparency=0.96*(1-i*0.3)end;af.BackgroundTransparency=0.85+(i*0.1)end end)end;local dA=function()if ac then ac:Disconnect();ac=nil end;if mc then mc:Disconnect();mc=nil end;if af then af:Destroy();af=nil end end;local ao={};function ao:Enable()if e then return end;cAF();e=true;uA()end;function ao:Disable()if not e then return end;dA();e=false end;function ao:Toggle()if e then self:Disable()else self:Enable()end end;function ao:SetIntensity(v)v=math.clamp(v,0,1);i=v;local bl=Lighting:FindFirstChild("AcrylicBlurEffect")if bl then bl.Size=12*v end;if af then local n=af:FindFirstChild("Noise")if n then n.ImageTransparency=0.96*(1-v*0.3)end;af.BackgroundTransparency=0.85+(v*0.1)end end;function ao:SetNoiseEnabled(en)ne=en;if af then local n=af:FindFirstChild("Noise")if n then n.Visible=en end end end;return ao end
function DiscordLib:SetTransparency(t)t=math.clamp(t,0,1);currentTransparency=t;local dg=game:GetService("CoreGui"):FindFirstChild("Discord")if not dg then return end;local mf=dg:FindFirstChild("MainFrame")if not mf then return end;mf.BackgroundTransparency=t;local shf=mf:FindFirstChild("ServersHoldFrame")if shf then shf.BackgroundTransparency=t end;local chf=mf:FindFirstChild("ChannelsHoldFrame")if chf then chf.BackgroundTransparency=t end;local up=mf:FindFirstChild("Userpad",true)if up then up.BackgroundTransparency=math.min(t+0.2,1)end;for _,sf in pairs(Sections)do if sf and sf.Parent then sf.BackgroundTransparency=t end end end
