function S:Textbox(t,d,ph,no,c)local bh,ty,dy,tby=58,8,26,45 if d then local tl=Instance.new("TextLabel")tl.Font,tl.TextSize,tl.Text,tl.Size,tl.TextWrapped,tl.RichText,tl.LineHeight=Enum.Font.Gotham,12,d,UDim2.new(0,340,0,1000),true,true,1.2 local th=tl.TextBounds.Y bh,dy,tby=58+th+8,26,45+th+5 tl:Destroy()end local tbf=Instance.new("Frame")tbf.Name,tbf.Parent,tbf.BackgroundColor3,tbf.BorderSizePixel,tbf.Size,tbf.ZIndex,tbf.ClipsDescendants="Textbox_"..t:gsub("%s+",""),sf,currentTheme.Tertiary,0,UDim2.new(0,370,0,bh),6,true local tbfc=Instance.new("UICorner")tbfc.CornerRadius,tbfc.Parent=UDim.new(0,4),tbf local tl=Instance.new("TextLabel")tl.Name,tl.Parent,tl.BackgroundColor3,tl.BackgroundTransparency,tl.Position,tl.Size,tl.Font,tl.Text,tl.TextColor3,tl.TextSize,tl.TextXAlignment,tl.TextYAlignment,tl.ZIndex="TextboxTitle",tbf,Color3.fromRGB(255,255,255),1,UDim2.new(0,10,0,ty),UDim2.new(0,250,0,18),Enum.Font.GothamBold,t,currentTheme.Text,14,Enum.TextXAlignment.Left,Enum.TextYAlignment.Center,7 local dl=Instance.new("TextLabel")dl.Name,dl.Parent,dl.BackgroundColor3,dl.BackgroundTransparency,dl.Position,dl.Size,dl.Font,dl.Text,dl.TextColor3,dl.TextSize,dl.TextXAlignment,dl.TextYAlignment,dl.ZIndex,dl.TextWrapped,dl.RichText,dl.LineHeight,dl.AutomaticSize="TextboxDesc",tbf,Color3.fromRGB(255,255,255),1,UDim2.new(0,10,0,dy),UDim2.new(0,340,0,bh-46),Enum.Font.Gotham,d,currentTheme.TextMuted,12,Enum.TextXAlignment.Left,Enum.TextYAlignment.Top,7,true,true,1.2,Enum.AutomaticSize.Y local tb=Instance.new("TextBox")tb.Name,tb.Parent,tb.BackgroundColor3,tb.BorderSizePixel,tb.Position,tb.Size,tb.Font,tb.PlaceholderText,tb.Text,tb.TextColor3,tb.PlaceholderColor3,tb.TextSize,tb.ClearTextOnFocus,tb.ZIndex="Textbox",tbf,currentTheme.Secondary,0,UDim2.new(0,10,0,tby),UDim2.new(0,350,0,24),Enum.Font.Gotham,ph,"",currentTheme.Text,currentTheme.TextMuted,13,false,7 local tbc=Instance.new("UICorner")tbc.CornerRadius,tbc.Parent=UDim.new(0,4),tb if no then tb.Text="0"end rtu(function()tbf.BackgroundColor3,tl.TextColor3,dl.TextColor3,tb.BackgroundColor3,tb.TextColor3,tb.PlaceholderColor3=currentTheme.Tertiary,currentTheme.Text,currentTheme.TextMuted,currentTheme.Secondary,currentTheme.Text,currentTheme.TextMuted end)tb.FocusLost:Connect(function(ep)if minimizeAnimating then return end if ep then if no then local n=tonumber(tb.Text)or 0 tb.Text=tostring(n)if c then c(n)end else if c then c(tb.Text)end end end end)sf.CanvasSize=UDim2.new(0,0,0,sl.AbsoluteContentSize.Y+40)local tbo={}function tbo:SetText(nt)tb.Text=tostring(nt)end function tbo:GetText()return tb.Text end function tbo:Clear()tb.Text=""end return tbo end
function S:Bind(t,dk,c)local bf,bl,bb=Instance.new("Frame"),Instance.new("TextLabel"),Instance.new("TextButton")local b,ck=false,dk or Enum.KeyCode.RightShift local kn=ck.Name or"RightShift"bf.Parent,bf.BackgroundColor3,bf.BorderSizePixel,bf.Size,bf.ZIndex=sf,currentTheme.Tertiary,0,UDim2.new(0,370,0,28),6 local bfc=Instance.new("UICorner")bfc.CornerRadius,bfc.Parent=UDim.new(0,4),bf bl.Parent,bl.BackgroundColor3,bl.BackgroundTransparency,bl.Position,bl.Size,bl.Font,bl.Text,bl.TextColor3,bl.TextSize,bl.TextXAlignment,bl.ZIndex=bf,Color3.fromRGB(255,255,255),1,UDim2.new(0,5,0,0),UDim2.new(0,260,0,28),Enum.Font.Gotham,t,currentTheme.Text,13,Enum.TextXAlignment.Left,7 bb.Parent,bb.BackgroundColor3,bb.BorderSizePixel,bb.Position,bb.Size,bb.Text,bb.Font,bb.TextColor3,bb.TextSize,bb.AutoButtonColor,bb.ZIndex=bf,currentTheme.Secondary,0,UDim2.new(0,280,0,3),UDim2.new(0,80,0,22),kn,Enum.Font.Gotham,currentTheme.Text,12,false,7 local bbc=Instance.new("UICorner")bbc.CornerRadius,bbc.Parent=UDim.new(0,3),bb rtu(function()bf.BackgroundColor3,bl.TextColor3,bb.BackgroundColor3,bb.TextColor3=currentTheme.Tertiary,currentTheme.Text,currentTheme.Secondary,currentTheme.Text end)bb.MouseButton1Click:Connect(function()if minimizeAnimating then return end b=true bb.Text="..."end)UserInputService.InputBegan:Connect(function(i,gp)if gp then return end if b and not minimizeAnimating then if i.UserInputType==Enum.UserInputType.Keyboard then ck=i.KeyCode kn=ck.Name bb.Text=kn b=false if c then c(ck)end end elseif i.KeyCode==ck and not minimizeAnimating then if c then c(ck)end end end)sf.CanvasSize=UDim2.new(0,0,0,sl.AbsoluteContentSize.Y+35)return bf end return S end
function Servers:Select(n)if minimizeAnimating then return end for _,v in pairs(ServersHold:GetChildren())do if v:IsA("TextButton")then TweenService:Create(v,TweenInfo.new(0.2),{BackgroundTransparency=(v.Name==n and 0 or 0.5)}):Play()end end currentServer,currentservertoggled=n,n for cn,cd in pairs(ServerChannels[n])do if cd.Button then cd.Button.Visible=true end end for os,ch in pairs(ServerChannels)do if os~=n then for _,cd in pairs(ch)do if cd.Button then cd.Button.Visible=false end end end end end
function Servers:SelectChannel(sn,cn)if minimizeAnimating then return end if sn and cn and ServerChannels[sn]and ServerChannels[sn][cn]then ServerChannels[sn][cn].Button.BackgroundColor3,ServerChannels[sn][cn].Button.BackgroundTransparency=currentTheme.Accent,0 currentChannel,currentchannettoggled=cn,cn local fn=sn..":"..cn for snm,s in pairs(Sections)do if s and s.Parent then s.Visible=(snm==fn)end end end end return{Server=Servers,Channel=Servers.Channel,CreateSection=Servers.Channel,SelectChannel=Servers.SelectChannel,SetTheme=DiscordLib.SetTheme}end
local activeNotifications,ns,bY={},80,10 local function rn()local y=bY for i,nd in ipairs(activeNotifications)do if nd.frame and nd.frame.Parent then TweenService:Create(nd.frame,TweenInfo.new(0.3,Enum.EasingStyle.Quad),{Position=UDim2.new(1,-310,0,y)}):Play()nd.yPosition=y y=y+nd.frame.AbsoluteSize.Y+10 else table.remove(activeNotifications,i)end end end
function DiscordLib:Notification(d)local t=d.title or"Notification"local de=d.desc or""local ti=d.time or5 local ny=bY for _,nd in ipairs(activeNotifications)do if nd.frame and nd.frame.Parent then ny=math.max(ny,nd.yPosition+nd.frame.AbsoluteSize.Y+10)end end local nh=Instance.new("Frame")nh.Name,nh.Parent,nh.BackgroundColor3,nh.BorderSizePixel,nh.Position,nh.Size,nh.ClipsDescendants,nh.BackgroundTransparency,nh.ZIndex,nh.AutomaticSize="Notification",Discord,currentTheme.Main,0,UDim2.new(1,-310,0,ny),UDim2.new(0,280,0,45),true,1,100,Enum.AutomaticSize.Y local nc=Instance.new("UICorner")nc.CornerRadius,nc.Parent=UDim.new(0,6),nh local nt=Instance.new("TextLabel")nt.Parent,nt.BackgroundColor3,nt.BackgroundTransparency,nt.Position,nt.Size,nt.Font,nt.Text,nt.TextColor3,nt.TextSize,nt.TextXAlignment,nt.TextTransparency,nt.ZIndex=nh,Color3.fromRGB(255,255,255),1,UDim2.new(0,10,0,8),UDim2.new(0,260,0,18),Enum.Font.GothamBold,t,currentTheme.Text,15,Enum.TextXAlignment.Left,1,101 local ndl=Instance.new("TextLabel")ndl.Parent,ndl.BackgroundColor3,ndl.BackgroundTransparency,ndl.Position,ndl.Size,ndl.Font,ndl.Text,ndl.TextColor3,ndl.TextSize,ndl.TextWrapped,ndl.TextXAlignment,ndl.TextYAlignment,ndl.TextTransparency,ndl.ZIndex,ndl.RichText,ndl.AutomaticSize,ndl.LineHeight=nh,Color3.fromRGB(255,255,255),1,UDim2.new(0,10,0,30),UDim2.new(0,260,0,16),Enum.Font.Gotham,de,currentTheme.TextMuted,13,true,Enum.TextXAlignment.Left,Enum.TextYAlignment.Top,1,101,true,Enum.AutomaticSize.Y,1.2 ndl:GetPropertyChangedSignal("AbsoluteSize"):Connect(function()nh.Size=UDim2.new(0,280,0,30+ndl.AbsoluteSize.Y+10)end)rtu(function()nh.BackgroundColor3,nt.TextColor3,ndl.TextColor3=currentTheme.Main,currentTheme.Text,currentTheme.TextMuted end)local ndd={frame=nh,yPosition=ny}table.insert(activeNotifications,ndd)nh.Position=UDim2.new(1,-310,0,ny-80)TweenService:Create(nh,TweenInfo.new(0.4,Enum.EasingStyle.Back,Enum.EasingDirection.Out),{Position=UDim2.new(1,-310,0,ny),BackgroundTransparency=0}):Play()TweenService:Create(nt,TweenInfo.new(0.3),{TextTransparency=0}):Play()TweenService:Create(ndl,TweenInfo.new(0.3),{TextTransparency=0}):Play()task.spawn(function()wait(ti)for i,d in ipairs(activeNotifications)do if d.frame==nh then table.remove(activeNotifications,i)break end end TweenService:Create(nh,TweenInfo.new(0.3,Enum.EasingStyle.Quad),{Position=UDim2.new(1,-310,0,ny-80),BackgroundTransparency=1}):Play()TweenService:Create(nt,TweenInfo.new(0.2),{TextTransparency=1}):Play()TweenService:Create(ndl,TweenInfo.new(0.2),{TextTransparency=1}):Play()task.wait(0.3)nh:Destroy()rn()end)return nh end
function DiscordLib:CreateAcrylicBlur()local ae,ai,ane,af,ac,mc=false,0.5,true,nil,nil,nil local RS=RunService local function caf()if af then return end local dg=game:GetService("CoreGui"):FindFirstChild("Discord")if not dg then return end local mf=dg:FindFirstChild("MainFrame")if not mf then return end af=Instance.new("Frame")af.Name,af.Parent,af.BackgroundColor3,af.BackgroundTransparency,af.BorderSizePixel,af.Size,af.Position,af.AnchorPoint,af.ZIndex,af.Active,af.ClipsDescendants="AcrylicBlur",dg,Color3.fromRGB(30,30,30),0.85,0,mf.Size,mf.Position,mf.AnchorPoint,0,false,true local c=Instance.new("UICorner")c.CornerRadius,c.Parent=UDim.new(0,4),af local g=Instance.new("UIGradient")g.Color,g.Rotation,g.Parent=ColorSequence.new({ColorSequenceKeypoint.new(0,Color3.fromRGB(40,40,40)),ColorSequenceKeypoint.new(1,Color3.fromRGB(20,20,20))}),90,af local n=Instance.new("ImageLabel")n.Name,n.Parent,n.BackgroundColor3,n.BackgroundTransparency,n.BorderSizePixel,n.Size,n.Position,n.ZIndex,n.Image,n.ImageColor3,n.ImageTransparency,n.ScaleType,n.TileSize="Noise",af,Color3.fromRGB(255,255,255),1,0,UDim2.new(1,0,1,0),UDim2.new(0,0,0,0),1,"rbxassetid://145514428",Color3.fromRGB(255,255,255),0.96,Enum.ScaleType.Tile,UDim2.new(0,128,0,128)mc=mf:GetPropertyChangedSignal("Position"):Connect(function()if af and af.Parent then af.Position=mf.Position end end)mf:GetPropertyChangedSignal("Size"):Connect(function()if af and af.Parent then af.Size=mf.Size end end)mf:GetPropertyChangedSignal("Visible"):Connect(function()if af and af.Parent then af.Visible=mf.Visible end end)mf:GetPropertyChangedSignal("AnchorPoint"):Connect(function()if af and af.Parent then af.AnchorPoint=mf.AnchorPoint end end)end local function ua()if ac then ac:Disconnect()end ac=RS.RenderStepped:Connect(function()if af and af.Parent and ae then local n=af:FindFirstChild("Noise")if n then n.ImageTransparency=0.96*(1-ai*0.3)end af.BackgroundTransparency=0.85+(ai*0.1)end end)end local function da()if ac then ac:Disconnect()ac=nil end if mc then mc:Disconnect()mc=nil end if af then af:Destroy()af=nil end end local ao={}function ao:Enable()if ae then return end caf()ae=true ua()end function ao:Disable()if not ae then return end da()ae=false end function ao:Toggle()if ae then ao:Disable()else ao:Enable()end end function ao:SetIntensity(i)i=math.clamp(i,0,1)ai=i local bl=Lighting:FindFirstChild("AcrylicBlurEffect")if bl then bl.Size=12*i end if af then local n=af:FindFirstChild("Noise")if n then n.ImageTransparency=0.96*(1-i*0.3)end af.BackgroundTransparency=0.85+(i*0.1)end end function ao:SetNoiseEnabled(e)ane=e if af then local n=af:FindFirstChild("Noise")if n then n.Visible=e end end end return ao end
function DiscordLib:SetTransparency(t)t=math.clamp(t,0,1)currentTransparency=t local dg=game:GetService("CoreGui"):FindFirstChild("Discord")if not dg then return end local mf=dg:FindFirstChild("MainFrame")if not mf then return end mf.BackgroundTransparency=t local sf=mf:FindFirstChild("ServersHoldFrame")if sf then sf.BackgroundTransparency=t end local cf=mf:FindFirstChild("ChannelsHoldFrame")if cf then cf.BackgroundTransparency=t end local up=mf:FindFirstChild("Userpad",true)if up then up.BackgroundTransparency=math.min(t+0.2,1)end for _,sf2 in pairs(Sections)do if sf2 and sf2.Parent then sf2.BackgroundTransparency=t end end end
createMinimizeButton()return DiscordLib
